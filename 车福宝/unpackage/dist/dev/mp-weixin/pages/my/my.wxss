
.icon_main{
	margin-top: 10rpx;
	width: 183rpx;
	height: 120rpx;
}
.icon_text{
	margin-top: 9rpx;
	font-size: 13px;
	width: 100%;
	text-align: center;
}
.icon_img{
	display: block;
	height: 65rpx;
	margin: 0 auto;
	width: 65rpx;
	object-fit: cover;
}
.icon{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	
	-webkit-flex-wrap: wrap;
	
	        flex-wrap: wrap;
}
.main_tit{
	padding-left: 23rpx;
	height: 65rpx;
	line-height: 65rpx;
	font-weight: bold;
	border-bottom: 6rpx #E5E5E5 solid;
}
.main{
	width: 740rpx;
	margin: 15rpx auto 0 auto;
	padding-top: 17rpx;
	padding-bottom: 17rpx;
	border-radius: 30rpx;
	overflow: hidden;
	background-color: #fff;
}
.img{
	object-fit: cover;
	width: 178rpx;
	height: 178rpx;
}
.header_img{
	width: 178rpx;
	height: 178rpx;
	border-radius: 50%;
	margin: 100rpx auto 0 auto;
	
	overflow: hidden;
}
.my_wrap{
	min-height: 100vh;
	background-color: #E5E5E5;
}
.header{
	overflow: hidden;
	margin: 0 auto;
	width: 740rpx;
	height: 376rpx;
	border-top-left-radius: 30rpx;
	border-top-right-radius: 30rpx;
	background-color: #5196e2;
}

